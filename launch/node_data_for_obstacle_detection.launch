<launch>
    <node name="node_data_for_obstacle_detection" pkg="draconis_demo_datafeed" type="node_data_for_obstacle_detection" output="screen">

        <param name="topic_velodyne_in" value="/$(env ROBOT_NAMESPACE)/velodyne_points" type="str" />
        <param name="topic_cloud_in" value="/$(env ROBOT_NAMESPACE)/l515/depth/color/points" type="str" />
        <param name="topic_odom_in" value="/$(env ROBOT_NAMESPACE)/odom" type="str" />
        <param name="topic_odom_t265_in" value="/$(env ROBOT_NAMESPACE)/t265/odom/sample" type="str" />
        <param name="topic_cloud_out" value="/$(env ROBOT_NAMESPACE)/l515_cloud" type="str" /> 

        <param name="map_frame_id" value="$(env ROBOT_NAMESPACE)/map" type="str" />
        <param name="base_link_frame_id" value="$(env ROBOT_NAMESPACE)/base_link" type="str" />
        <param name="cloud_out_frame_id" value="$(env ROBOT_NAMESPACE)/map" type="str" />
        <param name="init_transform_frame_id" value="$(env ROBOT_NAMESPACE)/l515_link" type="str"/>
        
        <param name="sampling_rate" value="4" type="double"/>
        <param name="leaf_size_down_sample" value="0.02" type="double"/>

        <param name="init_l515_trans_x" value="0.5" type="double"/>
        <param name="init_l515_trans_y" value="0.0" type="double"/>
        <param name="init_l515_trans_z" value="0.15" type="double"/>
        <param name="init_l515_rot_x" value="-16" type="double"/>
        <param name="init_l515_rot_y" value="0" type="double"/>
        <param name="init_l515_rot_z" value="1" type="double"/>

        <param name="init_lidar_trans_z" value="0.81" type="double"/>

        <param name="adjust_z_height" value="0.08" type="double"/>

        <param name="adjust_l515_trans_z" value="0.08" type="double"/>
        <param name="adjust_l515_rot_x" value="0.0" type="double"/>
        <param name="adjust_l515_rot_y" value="1.0" type="double"/>
        
    </node>
</launch>